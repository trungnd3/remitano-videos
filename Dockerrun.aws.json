{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "trunglinh53/remitano-videos-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "trunglinh53/remitano-videos-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "trunglinh53/remitano-videos-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}
